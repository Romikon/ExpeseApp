{
  "name": "expenseapp",
  "version": "1.0.0",
  "description": "<p align=\"center\">   <a href=\"http://nestjs.com/\" target=\"blank\"><img src=\"https://nestjs.com/img/logo-small.svg\" width=\"120\" alt=\"Nest Logo\" /></a> </p>",
  "main": ".eslintrc.js",
  "workspaces": [
    "apps/expense-transaction",
    "apps/expense-budget",
    "apps/expense-category",
    "apps/expense-user"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:user": "npm run start --workspace=apps/expense-user",
    "start:category": "npm run start --workspace=apps/expense-category",
    "start:transaction": "npm run start --workspace=apps/expense-transaction",
    "start:budget": "npm run start --workspace=apps/expense-budget",
    "start:all": "concurrently \"npm run start --workspace=apps/expense-transaction\" \"npm run start --workspace=apps/expense-budget\" \"npm run start --workspace=apps/expense-category\" \"npm run start --workspace=apps/expense-user\"",
    "migration:user": "npm run migration:run --workspace=apps/expense-user",
    "migration:budget": "npm run migration:run --workspace=apps/expense-budget",
    "migration:category": "npm run migration:run --workspace=apps/expense-category",
    "migration:transaction": "npm run migration:run --workspace=apps/expense-transaction",
    "migration:user:revert": "npm run migration:revert --workspace=apps/expense-user",
    "migration:budget:revert": "npm run migration:revert --workspace=apps/expense-budget",
    "migration:category:revert": "npm run migration:revert --workspace=apps/expense-category",
    "migration:transaction:revert": "npm run migration:revert --workspace=apps/expense-transaction",
    "migration:show:all": "concurrently \"npm run migration:show --workspace=apps/expense-transaction\" \"npm run migration:show --workspace=apps/expense-budget\" \"npm run migration:show --workspace=apps/expense-category\" \"npm run migration:show --workspace=apps/expense-user\"",
    "migration:show:user": "npm run migration:show --workspace=apps/expense-user",
    "migration:show:budget": "npm run migration:show --workspace=apps/expense-budget",
    "migration:show:category": "npm run migration:show --workspace=apps/expense-category",
    "migration:show:transaction": "npm run migration:show --workspace=apps/expense-transaction"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.1.1"
  },
  "dependencies": {
    "@nestjs/config": "^3.3.0",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  }
}